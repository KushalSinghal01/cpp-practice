#include <iostream>
using namespace std;
int main(){
    int n,i;
    cout<<"Enter the number: ";
    cin>>n;
    if(n<=1)
        cout<<"It is not a prime number..";
    else
        for(i=2;i<n;i++){
            if( n%i ==0)
                break;
        }
        if(i==n)
            cout<<"It is a prime number..";
        else
            cout<<"It is not a prime number..";
        
    return 0;
}
